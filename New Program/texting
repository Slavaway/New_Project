import unittest
import MainFunction

# Тестирую нахождение площади

class TestFind_S(unittest.TestCase):
    def test_add(self):
        self.assertEqual(MainFunction.add(3, 6), 18)


class TestFind_P(unittest.TestCase):

    def test_add(self):
        self.assertEqual(MainFunction.add(3, 6), 18)




if __name__ == "__main__":
    unittest.main()



